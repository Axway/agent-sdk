{
  "project": "APIC Agents SDK",
  "repo_url": "https://github.com/Axway/agent-sdk",
	"security_guide": "https://docs.axway.com/bundle/axway_resources/page/amplify_api_management_platform_security_white_paper.html",
  "requirements": {
    "dependency-check": false,
    "fortify": true,
    "irius-risk": false,
    "npm-audit": false,
    "pentest": false,
    "retirejs": false,
    "twistlock": false,
    "zap": false,
    "yarn": false,
    "gosec": false,
    "whitesource": true,
    "appspider": false,
    "insightvm": false
  },
  "suppressions": [
    {
      "path": ".*github.com\\/elastic\\/go\\-ucfg\\-v0\\.8\\.3",
      "scanner": "whitesource",
      "component": "github.com\\/elastic\\/go\\-ucfg",
      "version": "v0.8.3",
      "classification": "False Positive",
      "justification": "kubernetes libraries are not used by agents",
      "ids": [
        "CVE-2019-11254"
      ]
    }
  ]
}
