{
  "project": "APIC Agents SDK",
  "repo_url": "https://github.com/Axway/agent-sdk",
	"security_guide": "https://docs.axway.com/bundle/axway_resources/page/amplify_api_management_platform_security_white_paper.html",
  "requirements": {
    "dependency-check": false,
    "fortify": true,
    "irius-risk": false,
    "npm-audit": false,
    "pentest": false,
    "retirejs": false,
    "twistlock": false,
    "zap": false,
    "yarn": false,
    "gosec": false,
    "whitesource": true,
    "appspider": false,
    "insightvm": false
  },
  "suppressions": [
    {
      "path": "util/healthcheck/healthchecker\\.go:*",
      "scanner": "fortify",
      "classification": "Deferred",
      "until": "2022-01-07",
      "justification": "APIGOV-21114",
      "component": "healthchecker\\.go",
      "version": "master",
      "ids": [
        "Insecure Transport",
        "Privacy Violation",
        "JSON Injection"
      ]
    },
    {
      "path": "apic/auth/apicauth\\.go:*",
      "scanner": "fortify",
      "classification": "Deferred",
      "until": "2022-01-07",
      "justification": "APIGOV-21114",
      "component": "apicauth\\.go",
      "version": "master",
      "ids": [
        "Privacy Violation"
      ]
    }
  ]
}
